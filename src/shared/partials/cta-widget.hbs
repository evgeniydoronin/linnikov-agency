<div id="cta-widget" class="cta-widget {{class}}">
	<button type="button" class="cta-widget-btn" data-drawer-open="request">
		<svg class="cta-widget-arrow cta-widget__icon" viewBox="0 0 12 20" xmlns="http://www.w3.org/2000/svg">
			<g class="cta-widget-arrow__rgb cta-widget-arrow__rgb_red">
				<rect class="cta-widget-arrow__tip" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__tip" y="16" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__middle" x="4" y="4" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__middle" x="4" y="12" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__nose" x="8" y="8" width="4" height="4" fill="var(--color, currentColor)"/>
			</g>
			<g class="cta-widget-arrow__rgb cta-widget-arrow__rgb_green">
				<rect class="cta-widget-arrow__tip" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__tip" y="16" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__middle" x="4" y="4" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__middle" x="4" y="12" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__nose" x="8" y="8" width="4" height="4" fill="var(--color, currentColor)"/>
			</g>
			<g class="cta-widget-arrow__rgb cta-widget-arrow__rgb_blue">
				<rect class="cta-widget-arrow__tip" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__tip" y="16" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__middle" x="4" y="4" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__middle" x="4" y="12" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__nose" x="8" y="8" width="4" height="4" fill="var(--color, currentColor)"/>
			</g>
		</svg>
		<div class="cta-widget-btn__cap">Request</div>
	</button>
	<button type="button" class="cta-widget-btn" data-drawer-open="ai-chat">
		<div class="cta-widget-btn__cap">AI chat</div>
		<svg class="cta-widget-arrow cta-widget__icon" viewBox="0 0 12 20" xmlns="http://www.w3.org/2000/svg">
			<g class="cta-widget-arrow__rgb cta-widget-arrow__rgb_red">
				<rect class="cta-widget-arrow__tip" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__tip" y="16" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__middle" x="4" y="4" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__middle" x="4" y="12" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__nose" x="8" y="8" width="4" height="4" fill="var(--color, currentColor)"/>
			</g>
			<g class="cta-widget-arrow__rgb cta-widget-arrow__rgb_green">
				<rect class="cta-widget-arrow__tip" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__tip" y="16" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__middle" x="4" y="4" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__middle" x="4" y="12" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__nose" x="8" y="8" width="4" height="4" fill="var(--color, currentColor)"/>
			</g>
			<g class="cta-widget-arrow__rgb cta-widget-arrow__rgb_blue">
				<rect class="cta-widget-arrow__tip" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__tip" y="16" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__middle" x="4" y="4" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__middle" x="4" y="12" width="4" height="4" fill="var(--color, currentColor)"/>
				<rect class="cta-widget-arrow__nose" x="8" y="8" width="4" height="4" fill="var(--color, currentColor)"/>
			</g>
		</svg>
	</button>
</div>